{
    "name": "oberonamsterdam/travelbase-api-client",
    "description": "API library to communicate with Travelbase",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Oberon",
            "email": "contact@oberon.nl"
        },
        {
            "name": "Raymond Kiekens",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "gmostafa/php-graphql-client": "^1.4",
        "phpdocumentor/reflection-docblock": "^4.0|^5.0",
        "symfony/property-access": "^3.4|^4.1|^5.0",
        "symfony/property-info": "^3.4|^4.1|^5.0",
        "symfony/serializer": "^3.4|^4.1|^5.0"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "7.1"
        }
    },
    "autoload": {
        "psr-4": {
            "Oberon\\TravelbaseClient\\": "src/"
        }
    }
}
